{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "managedIdentity": {
      "value": {
        "name": "MediaPipeline",
        "resourceGroupName": "Azure.Media.Pipeline"
      }
    },
    "computeManager": {
      "value": {
        "image": {
          "publisher": "azurecyclecloud",
          "offer": "azure-cyclecloud",
          "sku": "cyclecloud-81",
          "version": "latest"
        },
        "machines": {
          "namePrefix": "computeManager",
          "size": "Standard_D4s_v3",
          "count": 1,
          "adminUsername": "az",
          "adminPassword": "P@ssword1234",
          "sshPublicKeyData": "ssh-rsa "
        },
        "loadBalancer": {
          "port": 443,
          "protocol": "Tcp"
        },
        "availabilitySet": {
          "faultDomains": 2,
          "updateDomains": 5
        }
      }
    },
    "storageAccount": {
      "value": {
        "name": "mediaasset",
        "resourceGroupName": "Azure.Media.Pipeline.Storage"
      }
    },
    "logAnalytics": {
      "value": {
        "deploy": false,
        "name": "MediaPipeline",
        "resourceGroupName": "Azure.Media.Pipeline"
      }
    },
    "virtualNetwork": {
      "value": {
        "name": "MediaPipelineEast",
        "subnetName": "Compute",
        "resourceGroupName": "Azure.Media.Pipeline.Network"
      }
    }
  }
}
