{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "managedIdentity": {
      "value": {
        "name": "MediaPipeline",
        "resourceGroupName": "Artist.Anywhere-Identity"
      }
    },
    "imageGallery": {
      "value": {
        "name": "MediaPipeline",
        "resourceGroupName": "Artist.Anywhere-Gallery"
      }
    },
    "virtualMachines": {
      "value": [
        {
          "deploy": false,
          "name": "ArtistNL",
          "size": "Standard_NV12s_v3",
          "image": {
            "definitionName": "WorkstationLinux",
            "versionId": "1.0.0"
          },
          "login": {
            "adminUsername": "az",
            "adminPassword": "P@ssword1234",
            "sshPublicKeyData": "ssh-rsa "
          }
        },
        {
          "deploy": false,
          "name": "ArtistNW",
          "size": "Standard_NV12s_v3",
          "image": {
            "definitionName": "WorkstationWindows",
            "versionId": "1.0.0"
          },
          "login": {
            "adminUsername": "az",
            "adminPassword": "P@ssword1234",
            "sshPublicKeyData": ""
          }
        }
      ]
    },
    "customExtension": {
      "value": {
        "linux": {
          "scriptFilePath": "/tmp/19-Machine.sh",
          "scriptParameters": "teradiciLicenseKey='' renderManagerHost=''"
        },
        "windows": {
          "scriptFilePath": "C:\\Windows\\Temp\\19-Machine.ps1",
          "scriptParameters": "-teradiciLicenseKey '' -renderManagerHost ''"
        },
        "scriptParameters": {
          "teradiciLicenseKey": "",
          "renderManagerHost": ""
        }
      }
    },
    "virtualNetwork": {
      "value": {
        "name": "MediaPipeline",
        "subnetName": "Workstation",
        "resourceGroupName": "Artist.Anywhere-Network"
      }
    }
  }
}
